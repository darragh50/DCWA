<head>
    <h1>Lecturers</h1>
</head>
<body>
    <a href="/">Home</a>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Department</th>
            </tr>
        </thead>
        <tbody>
            <% lecturers.forEach(lecturer => { %>
                <tr>
                    <td><%= lecturer._id %></td>
                    <td><%= lecturer.name %></td>
                    <td><%= lecturer.did %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>